{
    "name": "StaticModule",
    "moduleName": "StaticModule",
    "modelName": "StaticBlock",
    "namespace": "staticblocks",
    "collectionName": "StaticBlocks",
    "url": "/staticblocks/",
    "urlRoot": "staticblocks",
    "dbTable": "staticblocks",
    "navigation": true,
    "notpublishable": true,
    "fields": ["title", "image", "layout", "data"],
    "columns": ["title"],
    "settings": {
      "layout" : {
        "value": "default, noimage, imageleft, imageright",
        "type": "text"
      }
    },
    "model": {
        "title" : {
          "type": "text",
          "required": true,
          "mongooseType": "String"
        },
        "image" : {
          "type": "file",
          "multiple": false
        },
        "layout": {
          "type": "select",
          "setting": "layout"
        },
        "data" : {
          "type": "wysiwyg"
        }
    }
}
